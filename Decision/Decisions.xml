<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <policies>
    <item key="A" value="Decision.Policies.AlphaPolicy" />
    <item key="B" value="Decision.Policies.BetaPolicy" />
    <item key="C" value="Decision.Policies.GammaPolicy" />
  </policies>
  <expressions provider="Decision.ExpressionProvider">
    <item key="U" value="!A" />
    <item key="V" value="!(!!(!C))" />
    <item key="W" value="(!A AND B) OR (A AND !B)" />
    <item key="X" value="(A OR C) AND B" />
    <item key="Y" value="(A . C) | (A AND B) OR (A . B &amp; C)" />
    <item key="Z" value="(B)" />
  </expressions>
</configuration>